    "storage",
    "supabase",
    "tiny-cms"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/storage-js": "^2.80.0"
  },
  "peerDependencies": {
    "@tiny-cms/core": "^0.1.0",
    "hono": "^4.5.0"
  },
  "devDependencies": {
    "@tiny-cms/core": "workspace:*"
  }
