{
  "name": "@tiny-cms/ui",
  "version": "0.1.0",
  "description": "UI components for tiny-cms admin interface",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsc && tailwindcss -i ./src/styles.css -o ./dist/styles.css --minify",
    "dev": "tsc --watch",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cms",
    "ui",
    "admin",
    "tiny-cms"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tiny-cms/core": "workspace:*",
    "@uiw/react-md-editor": "^4.0.4",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "react-hook-form": "^7.53.2",
    "@hookform/resolvers": "^3.9.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.5"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.17",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  }
}
