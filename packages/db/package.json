{
  "name": "@tiny-cms/db",
  "version": "0.1.0",
  "description": "Database adapter for tiny-cms with Kysely and PostgreSQL",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cms",
    "database",
    "kysely",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tiny-cms/core": "workspace:*",
    "kysely": "^0.27.4",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10"
  }
}
