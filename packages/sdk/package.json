{
  "name": "@tiny-cms/sdk",
  "version": "0.1.0",
  "description": "Type-safe client SDK for tiny-cms",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cms",
    "sdk",
    "client",
    "tiny-cms"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@tiny-cms/core": "workspace:*"
  }
}
